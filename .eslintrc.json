{
  "extends": ["@9renpoto/eslint-config"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "no-undef": 1,
    "no-unused-vars": 1
  },
  "env": {
    "mocha": true
  }
}
